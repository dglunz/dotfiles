# Curl Aliases
curlp() {
  curl -X POST -H "Content-Type: application/json" -d "@./tmp/$1.json" "http://localhost:3000/$2"
  curl -X POST -H "Authorization: Bearer $PANA_ADMIN" -H "Content-Type: application/json" -d "@./tmp/$1.json" "http://localhost:3000/$2"
}

